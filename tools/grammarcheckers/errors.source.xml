<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE errors>

<errors>

  <defaults>
    <default type="Grammar error">
      <ids>
        <re v="real-.*"/>
        <e id="unreal-girjji"/>
      </ids>
      <header>
        <title xml:lang="en">Real-word error</title>
        <title xml:lang="__UND2C__">Real-word error in __UND2C__ (PLACEHOLDER)</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" exists in the dictionary, but means something different in this context.</description>
        <description xml:lang="__UND2C__">Real-word error description for $1 in __UND2C__ (PLACEHOLDER)</description>
      </body>
    </default>

    <default type="Grammar error">
      <ids>
        <e id="default"/>
      </ids>
      <header>
        <title xml:lang="en">Grammar error</title>
        <title xml:lang="__UND2C__">Grammar error in __UND2C__</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" exists in the dictionary, but means something different in this context.</description>
        <description xml:lang="__UND2C__">Grammar error description for $1 in __UND2C__ (PLACEHOLDER)</description>
      </body>
    </default>
  </defaults>

  <error id="real-override">
